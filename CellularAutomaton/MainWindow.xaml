<Window x:Class="CellularAutomaton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"


        xmlns:local="clr-namespace:CellularAutomaton"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1300" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0">
        <Grid>
            <Menu IsMainMenu="true">
                <MenuItem Header="GrainGrowthPage"  Click="MenuItem_Click_2"/>
                <MenuItem Visibility="Hidden" Header="One dimension Automat" Click="MenuItem_Click"/>
                <MenuItem Visibility="Hidden" Header="Game Of Life" Click="MenuItem_Click_1"/>

            </Menu>
        </Grid>
        <Grid Margin="0,20,0,0">
            <Frame x:Name="Frame"  NavigationUIVisibility="Hidden" />
        </Grid>
    </Grid>
</Window>
